<?xml version="1.0" encoding="utf-8" ?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dbtest.dbkeshe.repository.VideoInfoRepository">

    <select id="findAll" resultMap="videoEntity" >
        select * from historyvideo as h,kitchen as k
        where h.id = k.id
    </select>
    <select id="count" resultType="int">
        select count(*) from historyvideo
    </select>
    <resultMap id="videoEntity" type="VideoInfo">
        <id property="videoId" column="videoid"></id>
        <result property="id" column="id"></result>
        <result property="videoURL" column="videoURL"></result>
        <result property="date" column="date"></result>
        <association property="kitchen" javaType="com.dbtest.dbkeshe.entity.Kitchen">
            <id property="id" column="id"></id>
            <result property="name" column="name"></result>
            <result property="location" column="location"></result>
        </association>
    </resultMap>


</mapper>